{
	"cli": {
	  "version": ">= 14.7.1",
	  "appVersionSource": "remote"
	},
	"build": {
	  "development": {
		"developmentClient": true,
		"distribution": "internal",
		"android": {
		  "buildType": "apk"
		}, 
		"hooks": {
        "post-build": "./eas-build-post-run.sh"
      }
	  },
	  "preview": {
		"developmentClient": false,
		"distribution": "internal",
		"android": {
		  "buildType": "apk"
		}  
	  },
	  "production": {
		"autoIncrement": true,
		"android": {
		  "buildType": "apk"
		}
	  }
	},
	"submit": {
	  "production": {}
	}
  }
  